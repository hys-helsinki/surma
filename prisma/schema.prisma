// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id                  String @id @default(uuid())
  alias               String
  firstName           String
  lastName            String
  phone               String
  email               String
  address             String
  learningInstitution String
  eyeColor            String
  hair                String
  height              Int
  glasses             String
  other               String
  calendar            Json
}
